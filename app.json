{
  "expo": {
    "name": "Corolla Ice Delivery",
    "owner": "corollaicedelivery",
    "slug": "ice-delivery",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./app/assets/iphone-splash-preview.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
