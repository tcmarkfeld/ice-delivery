    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.corollaicedelivery.icedelivery"
    },
    "android": {},
    "web": {},
    "plugins": [
      "expo-secure-store"
    ],
    "extra": {
      "eas": {
        "projectId": "f53a6869-59e8-44c9-9d94-c6838fc0572d"
      }
    }
  }
}
